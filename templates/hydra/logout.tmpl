{{ define "hydra/logout" }}
<html>

<head>
    <title>{{ .title }}</title>
</head>

<body>
<h1>Do you wish to log out?</h1>{{if .error}}
    <p>{{.error}}</p>
{{end}}
<form action="/logout" method="POST"><input type="hidden" name="_csrf" value="{{.csrfToken}}"><input type="hidden" name="challenge" value="{{.challenge}}"><input type="submit" id="accept" value="Yes"><input type="submit" id="reject" value="No"></form>

</body>

</html>
{{ end }}
